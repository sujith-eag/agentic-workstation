{
  "$schema": "../../_canonical_schemas/agents.schema.json",
  "workflow_id": "implementation",
  "agents": [
    {
      "id": "I-00",
      "slug": "orchestrator",
      "role": "Implementation Orchestrator",
      "agent_type": "orchestrator",
      "description": "Session controller for implementation phase. Manages task assignment, handoffs, project index, and gating checks.",
      "produces": {
        "core": ["implementation_index.md", "active_session.md"]
      },
      "consumes": {
        "gating": [
          "implementation_readiness_report.md",
          "implementation_handoff_plan.md",
          "implementation_task_list.md"
        ]
      }
    },
    {
      "id": "I-01",
      "slug": "scaffold",
      "role": "Scaffold & DevOps Architect",
      "agent_type": "core",
      "description": "Sets up project structure, CI/CD, tooling, and infrastructure. Documents setup and infrastructure decisions.",
      "produces": {
        "core": [
          "codebase_scaffold_map.md",
          "SETUP.md",
          "INFRASTRUCTURE.md"
        ],
        "domain": [
          "cicd_config/",
          "infrastructure/",
          "linter_configs/"
        ]
      },
      "consumes": {
        "core": [
          "implementation_task_list.md",
          "api_specifications.md",
          "architecture_overview.md"
        ],
        "domain": [
          "infrastructure_plan.md",
          "cicd_pipeline_plan.md",
          "environment_strategy.md"
        ],
        "reference": [
          "deployment_model.md",
          "cloud_cost_projection.md"
        ],
        "gating": [
          "implementation_feasibility_gap_report.md",
          "integration_consistency_audit.md"
        ]
      }
    },
    {
      "id": "I-02",
      "slug": "backend",
      "role": "Backend & Data Engineer",
      "agent_type": "core",
      "description": "Implements backend logic, APIs, data layer, and migrations. Documents APIs and data schemas.",
      "produces": {
        "core": [
          "api_realization_status.md",
          "backend_test_coverage.md"
        ]
      },
      "consumes": {
        "core": [
          "implementation_task_list.md",
          "api_specifications.md",
          "traceability_matrix.md"
        ],
        "domain": [
          "business_logic_map.md",
          "logical_data_model.md",
          "data_strategy.md"
        ],
        "reference": [
          "integration_plan.md",
          "communication_flows.md",
          "data_governance_plan.md"
        ],
        "gating": [
          "implementation_feasibility_gap_report.md",
          "integration_consistency_audit.md"
        ]
      }
    },
    {
      "id": "I-03",
      "slug": "frontend",
      "role": "Frontend & UX Engineer",
      "agent_type": "core",
      "description": "Implements UI components, interactions, and accessibility. Documents components and UI patterns.",
      "produces": {
        "core": [
          "ui_component_inventory.md",
          "frontend_test_coverage.md"
        ]
      },
      "consumes": {
        "core": [
          "implementation_task_list.md",
          "api_specifications.md",
          "traceability_matrix.md"
        ],
        "domain": [
          "conceptual_wireframes.md",
          "interaction_flows.md",
          "information_architecture.md",
          "accessibility_compliance_plan.md"
        ],
        "reference": [
          "user_journeys.md",
          "ux_principles_and_goals.md"
        ],
        "gating": [
          "implementation_feasibility_gap_report.md",
          "integration_consistency_audit.md"
        ]
      }
    },
    {
      "id": "I-04",
      "slug": "quality",
      "role": "Quality & Validation Specialist",
      "agent_type": "core",
      "description": "Testing (all levels), code review, observability instrumentation. Documents test coverage and quality metrics.",
      "produces": {
        "core": [
          "test_execution_register.md",
          "code_review_report.md",
          "metrics_catalog.md",
          "instrumentation_status.md",
          "coverage_report.md"
        ]
      },
      "consumes": {
        "core": [
          "implementation_task_list.md",
          "requirements_spec.md",
          "traceability_matrix.md"
        ],
        "domain": [
          "testing_strategy.md",
          "test_architecture_plan.md",
          "nonfunctional_validation_plan.md"
        ],
        "reference": [
          "resilience_architecture_plan.md",
          "quality_governance.md"
        ]
      }
    },
    {
      "id": "I-05",
      "slug": "release",
      "role": "Release & Integration Manager",
      "agent_type": "core",
      "description": "Prepares release packages, changelog, and deployment verification.",
      "produces": {
        "core": [
          "CHANGELOG.md",
          "release_notes.md",
          "deployment_verification_log.md"
        ]
      },
      "consumes": {
        "core": [
          "implementation_task_list.md",
          "test_execution_register.md",
          "traceability_matrix.md"
        ],
        "domain": [
          "transition_readiness_summary.md",
          "implementation_handoff_plan.md"
        ],
        "reference": [
          "deployment_model.md",
          "environment_strategy.md"
        ],
        "gating": [
          "implementation_feasibility_gap_report.md",
          "integration_consistency_audit.md"
        ]
      }
    },
    {
      "id": "I-SEC",
      "slug": "security",
      "role": "Security Hardening Auditor",
      "agent_type": "on_demand",
      "description": "Security review and vulnerability scanning. Invokable at any stage for security analysis.",
      "produces": {
        "core": [
          "security_audit_report.md",
          "vulnerability_scan.md",
          "dependency_audit.md",
          "security_signoff.md"
        ]
      },
      "consumes": {
        "core": [
          "secure_design_recommendations.md",
          "security_control_trace.md"
        ]
      }
    },
    {
      "id": "I-DOC",
      "slug": "documentation",
      "role": "Documentation Sprint Agent",
      "agent_type": "on_demand",
      "description": "Comprehensive documentation beyond agent defaults. Architecture diagrams, API references, user guides.",
      "produces": {
        "core": [
          "docs/ARCHITECTURE.md",
          "docs/API_REFERENCE.md",
          "docs/USER_GUIDE.md",
          "docs/DEVELOPER_GUIDE.md",
          "docs/DEPLOYMENT_GUIDE.md"
        ]
      },
      "consumes": {
        "core": [
          "architecture_overview.md",
          "api_specifications.md"
        ]
      }
    },
    {
      "id": "I-DS",
      "slug": "data_steward",
      "role": "Data Store Implementation Specialist",
      "agent_type": "on_demand",
      "description": "Complex data migrations, PII handling, data governance.",
      "produces": {
        "core": [
          "migration_safety_checklist.md",
          "data_change_impact_log.md",
          "pii_handling_register.md",
          "data_quality_scorecard.md"
        ]
      },
      "consumes": {
        "core": [
          "logical_data_model.md",
          "data_governance_plan.md",
          "data_lifecycle_plan.md"
        ]
      }
    },
    {
      "id": "I-PERF",
      "slug": "performance",
      "role": "Performance Optimization Agent",
      "agent_type": "on_demand",
      "description": "Performance profiling, optimization, load testing.",
      "produces": {
        "core": [
          "performance_report.md",
          "optimization_log.md",
          "load_test_results.md",
          "performance_baseline.md"
        ]
      },
      "consumes": {
        "core": [
          "nonfunctional_validation_plan.md",
          "capacity_planning_model.md"
        ]
      }
    }
  ]
}
