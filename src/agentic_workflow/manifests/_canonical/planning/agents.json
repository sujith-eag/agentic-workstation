{
  "$schema": "../../_canonical_schemas/agents.schema.json",
  "workflow_id": "planning",
  "agents": [
    {
      "id": "A-00",
      "slug": "orchestrator",
      "role": "Orchestrator & Project Controller",
      "agent_type": "orchestrator",
      "description": "System admin. Manages session lifecycle, index maintenance, and base context.",
      "produces": {
        "core": ["project_index.md", "active_session.md"]
      },
      "consumes": {
        "core": []
      }
    },
    {
      "id": "A-01",
      "slug": "incubation",
      "role": "Project Guide & Idea Incubation",
      "agent_type": "core",
      "description": "Refines raw concepts into a structured seed.",
      "produces": {
        "core": ["project_overview_seed.md"],
        "log": ["idea_evolution_log.md"]
      },
      "consumes": {
        "core": []
      }
    },
    {
      "id": "A-02",
      "slug": "planning",
      "role": "Planning & Requirements Analyst",
      "agent_type": "core",
      "description": "Establishes scope, functional/non-functional requirements, constraints, feasibility, and initial traceability seed.",
      "produces": {
        "core": ["project_overview.md", "requirements_spec.md", "constraints_and_assumptions.md", "traceability_matrix.md"],
        "log": ["context_log.md", "exchange_log.md"]
      },
      "consumes": {
        "core": [
          { "file": "project_overview_seed.md", "required": false }
        ]
      }
    },
    {
      "id": "A-03",
      "slug": "architecture",
      "role": "Architecture & Technical Design Analyst",
      "agent_type": "core",
      "description": "Defines structural system design, component boundaries, data flows, architectural trade-offs, and stack recommendations.",
      "produces": {
        "core": ["architecture_overview.md", "system_design.md", "component_diagrams.md", "data_flow_overview.md", "adr_index.md"]
      },
      "consumes": {
        "core": ["project_overview.md", "requirements_spec.md"]
      }
    },
    {
      "id": "A-04",
      "slug": "security",
      "role": "Security & Compliance Scrutineer",
      "agent_type": "core",
      "description": "Evaluates threat models, compliance obligations, and maps security controls to requirements.",
      "produces": {
        "core": ["security_threat_assessment.md", "compliance_requirements.md", "privacy_policy_considerations.md", "secure_design_recommendations.md"]
      },
      "consumes": {
        "core": ["architecture_overview.md", "system_design.md", "data_flow_overview.md"]
      }
    },
    {
      "id": "A-05",
      "slug": "infra",
      "role": "Infrastructure & DevOps Planning Architect",
      "agent_type": "core",
      "description": "Defines conceptual environment topology, provisioning models, CI/CD pipeline plans, deployment strategies, and cost considerations.",
      "produces": {
        "core": ["infrastructure_plan.md", "environment_strategy.md", "cicd_pipeline_plan.md", "deployment_model.md", "cloud_cost_projection.md"]
      },
      "consumes": {
        "core": ["architecture_overview.md", "system_design.md", "security_threat_assessment.md"]
      }
    },
    {
      "id": "A-06",
      "slug": "data",
      "role": "Data Architecture & Storage Planning Specialist",
      "agent_type": "core",
      "description": "Designs logical data models, storage strategies, lifecycle rules, and data governance alignment.",
      "produces": {
        "core": ["data_strategy.md", "logical_data_model.md", "storage_strategy.md", "data_lifecycle_plan.md", "data_governance_plan.md"]
      },
      "consumes": {
        "core": ["requirements_spec.md", "system_design.md", "data_flow_overview.md", "security_threat_assessment.md"]
      }
    },
    {
      "id": "A-07",
      "slug": "api",
      "role": "API & Integration Planning Specialist",
      "agent_type": "core",
      "description": "Plans API strategy, conceptual endpoints, integration boundaries, interaction patterns, and communication flows.",
      "produces": {
        "core": ["api_specifications.md", "api_strategy_overview.md", "integration_plan.md", "communication_flows.md"]
      },
      "consumes": {
        "core": ["system_design.md", "data_strategy.md", "component_diagrams.md"]
      }
    },
    {
      "id": "A-08",
      "slug": "ux",
      "role": "UX & Interaction Planning Specialist",
      "agent_type": "core",
      "description": "Develops personas, user journeys, information architecture, and conceptual wireframes.",
      "produces": {
        "core": ["ux_principles_and_goals.md", "information_architecture.md", "user_personas_and_roles.md", "user_journeys.md", "interaction_flows.md", "conceptual_wireframes.md", "accessibility_compliance_plan.md"],
        "log": ["ux_log.md"]
      },
      "consumes": {
        "core": ["project_overview.md", "requirements_spec.md", "api_specifications.md", "data_strategy.md"]
      }
    },
    {
      "id": "A-09",
      "slug": "devworkflow",
      "role": "Developer Workflow & Engineering Standards Planner",
      "agent_type": "core",
      "description": "Sets branching strategies, coding standards, documentation rules, test boundaries, and governance policies.",
      "produces": {
        "core": ["development_workflow.md", "engineering_standards.md", "testing_strategy.md", "developer_tooling_and_environment.md", "engineering_governance.md", "technology_watchlist.md"],
        "log": ["dev_workflow_log.md"]
      },
      "consumes": {
        "core": [
          "project_overview.md",
          "system_design.md",
          "api_specifications.md",
          "cicd_pipeline_plan.md",
          "secure_design_recommendations.md",
          { "file": "information_architecture.md", "required": false }
        ]
      }
    },
    {
      "id": "A-10",
      "slug": "qa",
      "role": "QA & Test Strategy Designer",
      "agent_type": "core",
      "description": "Defines test strategy, coverage models, test architecture, risk-based prioritization, and non-functional validation plans.",
      "produces": {
        "core": ["qa_strategy_overview.md", "test_coverage_model.md", "test_architecture_plan.md", "test_plan_and_prioritization.md", "quality_governance.md", "nonfunctional_validation_plan.md"],
        "log": ["qa_log.md"]
      },
      "consumes": {
        "core": ["requirements_spec.md", "system_design.md", "api_specifications.md", "information_architecture.md", "testing_strategy.md"]
      }
    },
    {
      "id": "A-11",
      "slug": "product",
      "role": "Product Alignment & Business Logic Planner",
      "agent_type": "core",
      "description": "Validates technical plans against product strategy/KPIs and surfaces trade-offs between cost, time-to-market, and value.",
      "produces": {
        "core": ["product_alignment_report.md", "feature_priority_matrix.md", "business_logic_map.md"],
        "log": ["product_alignment_log.md"]
      },
      "consumes": {
        "core": ["project_overview.md", "requirements_spec.md", "architecture_overview.md", "system_design.md", "api_specifications.md"]
      }
    },
    {
      "id": "A-12",
      "slug": "sre",
      "role": "SRE, Reliability & Resilience Planning Agent",
      "agent_type": "core",
      "description": "Plans operational reliability (SLI/SLO), resilience patterns, capacity planning, and incident management readiness.",
      "produces": {
        "core": ["sre_readiness_plan.md", "resilience_architecture_plan.md", "capacity_planning_model.md"],
        "gating": ["implementation_readiness_report.md"],
        "log": ["sre_log.md"]
      },
      "consumes": {
        "core": [
          "architecture_overview.md",
          "infrastructure_plan.md",
          "data_strategy.md",
          "secure_design_recommendations.md",
          { "file": "requirements_spec.md", "required": false },
          { "file": "deployment_model.md", "required": false },
          { "file": "cloud_cost_projection.md", "required": false },
          { "file": "system_design.md", "required": false }
        ]
      }
    },
    {
      "id": "A-13",
      "slug": "transition",
      "role": "Implementation Transition & Completeness Auditor",
      "agent_type": "gating",
      "description": "Performs final completeness verification, reconciles traceability, and prepares the repository for the build phase.",
      "produces": {
        "core": ["transition_readiness_summary.md", "implementation_handoff_plan.md", "specs/backend_spec.md", "specs/frontend_spec.md", "specs/infrastructure_spec.md", "specs/test_spec.md"],
        "log": ["transition_audit_log.md"]
      },
      "consumes": {
        "gating": ["implementation_readiness_report.md"],
        "log": ["exchange_log.md"],
        "core": [
          "traceability_matrix.md",
          "adr_index.md",
          { "file": "sre_readiness_plan.md", "required": false },
          { "file": "resilience_architecture_plan.md", "required": false },
          { "file": "capacity_planning_model.md", "required": false },
          { "file": "quality_governance.md", "required": false },
          { "file": "testing_strategy.md", "required": false },
          { "file": "security_control_trace.md", "required": false }
        ]
      }
    },
    {
      "id": "A-14",
      "slug": "audit",
      "role": "Implementation Feasibility & Integration Integrity Auditor",
      "agent_type": "gating",
      "description": "Performs cross-artifact feasibility, integration consistency, logic completeness, and deprecation exposure audit.",
      "produces": {
        "gating": ["implementation_feasibility_gap_report.md", "integration_consistency_audit.md", "logic_completeness_review.md", "deprecation_risk_matrix.md"],
        "log": ["implementation_scrutiny_log.md"]
      },
      "consumes": {
        "core": [
          "project_overview.md",
          "requirements_spec.md",
          "constraints_and_assumptions.md",
          "architecture_overview.md",
          "system_design.md",
          "data_flow_overview.md",
          "logical_data_model.md",
          "data_strategy.md",
          "api_specifications.md",
          "integration_plan.md",
          "communication_flows.md",
          "business_logic_map.md",
          "traceability_matrix.md",
          "adr_index.md",
          { "file": "component_diagrams.md", "required": false },
          { "file": "secure_design_recommendations.md", "required": false },
          { "file": "sre_readiness_plan.md", "required": false },
          { "file": "resilience_architecture_plan.md", "required": false },
          { "file": "capacity_planning_model.md", "required": false },
          { "file": "test_coverage_model.md", "required": false },
          { "file": "information_architecture.md", "required": false },
          { "file": "interaction_flows.md", "required": false },
          { "file": "technology_watchlist.md", "required": false },
          { "file": "accessibility_compliance_plan.md", "required": false },
          { "file": "implementation_handoff_plan.md", "required": false },
          { "file": "transition_readiness_summary.md", "required": false }
        ],
        "gating": ["implementation_readiness_report.md"],
        "log": [
          { "file": "exchange_log.md", "required": false },
          { "file": "context_log.md", "required": false },
          { "file": "ux_log.md", "required": false },
          { "file": "dev_workflow_log.md", "required": false },
          { "file": "qa_log.md", "required": false },
          { "file": "product_alignment_log.md", "required": false },
          { "file": "sre_log.md", "required": false },
          { "file": "transition_audit_log.md", "required": false }
        ]
      }
    }
  ]
}
